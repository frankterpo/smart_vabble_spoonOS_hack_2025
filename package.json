{
  "name": "vabs_v2",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "compile": "bash scripts/neo-workflow/compile.sh",
    "compile:invoice": "bash scripts/neo-workflow/compile-invoice.sh",
    "compile:investor": "bash scripts/neo-workflow/compile-investor.sh",
    "compile:registry": "bash scripts/neo-workflow/compile-registry.sh",
    "compile:exporter": "bash scripts/neo-workflow/compile-exporter.sh",
    "compile:importer": "bash scripts/neo-workflow/compile-importer.sh",
    "compile:dd": "bash scripts/neo-workflow/compile-dd.sh",
    "compile:all": "npm run compile:invoice && npm run compile:investor && npm run compile:registry && npm run compile:exporter && npm run compile:importer && npm run compile:dd",
    "generate-wallet": "bash scripts/neo-workflow/generate-wallet.sh",
    "deploy": "node scripts/neo-workflow/deploy.js",
    "deploy:local": "node scripts/neo-workflow/deploy-local.js",
    "deploy:all:local": "node scripts/neo-workflow/deploy-all-local.js",
    "deploy:invoice:local": "node scripts/neo-workflow/deploy-invoice-local.js",
    "deploy:investor:local": "node scripts/neo-workflow/deploy-investor-local.js",
    "deploy:registry:local": "node scripts/neo-workflow/deploy-registry-local.js",
    "deploy:exporter": "node scripts/neo-workflow/deploy-exporter-testnet.js",
    "deploy:importer": "node scripts/neo-workflow/deploy-importer-testnet.js",
    "deploy:dd": "node scripts/neo-workflow/deploy-dd-testnet.js",
    "invoke": "node scripts/neo-workflow/invoke.js",
    "invoke:invoice": "node scripts/neo-workflow/invoke-invoice.js",
    "invoke:investor": "node scripts/neo-workflow/invoke-investor.js",
    "invoke:registry": "node scripts/neo-workflow/invoke-registry.js",
    "invoke:settlement": "node scripts/neo-workflow/settlement-coordinator.js",
    "backend": "npx tsc && node dist/server.cjs",
    "backend:local": "npx tsc && NEO_NETWORK=local node dist/server.cjs",
    "backend:testnet": "npx tsc && NEO_NETWORK=testnet node dist/server.cjs",
    "demo:local": "npm run backend:local",
    "demo:testnet": "npm run backend:testnet",
    "neoxp:start": "DOTNET_ROOT=$HOME/.dotnet DOTNET_MULTILEVEL_LOOKUP=0 $HOME/.dotnet/tools/neoxp run --seconds-per-block 1",
    "neoxp:fund": "DOTNET_ROOT=$HOME/.dotnet DOTNET_MULTILEVEL_LOOKUP=0 $HOME/.dotnet/tools/neoxp transfer 1000 GAS genesis NQGop9Bnj21Zq8foruvzsa1v7vxHiiRZEb",
    "agent": "cd scripts/agent && python3 vabble_agent_v2.py",
    "agent:demo": "cd scripts/agent && python3 vabble_agent_v2.py demo"
  },
  "dependencies": {
    "@cityofzion/neon-js": "^5.5.0",
    "chalk": "^5.6.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "body-parser": "^2.2.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
